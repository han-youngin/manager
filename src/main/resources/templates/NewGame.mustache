{{>/Header}}

<div class="container" style="margin-top: 70px;">
  <h2>신규 게임 등록</h2>
  <form action="/create" method="POST">
    <div class="form-group">
      <label for="email">게임 이름:</label>
      <input type="text" class="form-control" id="gamename" placeholder="게임 이름을 입력하세요" name="gamename">
    </div>
    <form method="post" enctype="multipart/form-data">
      <div class="button">
          <label for="chooseFile">
              등록할 이미지를 선택하세요:
          </label>
      </div>
      <input type="file" id="chooseFile" name="chooseFile" accept="image/*" onchange="loadFile(this)">
  </form>

    <button type="submit" class="btn btn-primary" id="btn-create">등록</button>
  </form>
</div>



</body>
</html>